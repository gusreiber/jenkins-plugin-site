    "url": "https://issues.jenkins-ci.org/secure/RapidBoard.jspa?rapidView=1&projectKey=WEBSITE&view=detail"
  },
  "homepage": "http://plugins.jenkins-ci.org/",
  "dependencies": {
    "async": "2.0.0-rc.3",
    "classnames": "2.2.5",
    "history": "2.1.1",
    "htmlparser2": "^3.9.0",
    "immutable": "3.8.1",
    "keymirror": "0.1.1",
    "lodash": "4.11.2",
    "moment": "2.13.0",
